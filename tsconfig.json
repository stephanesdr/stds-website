{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@interfaces/*": ["src/interfaces/*"],
      "@layouts/*": ["src/layouts/*"],
      "@pages/*": ["src/pages/*"],
      "@images/*": ["src/images/*"]
    },
    "target": "es6",
    "jsx": "react-jsx",
    "jsxImportSource": "react"
}
}