{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@interfaces/*": ["src/interfaces/*"],
      "@layouts/*": ["src/layouts/*"],
      "@stores/*": ["src/stores/*"],
      "@pages/*": ["src/pages/*"],
      "@utils/*": ["src/utils/*"],
      "@images/*": ["src/images/*"],
      "@data/*": ["src/data/*"]
     
    },
    "target": "es2017",
    "jsx": "react-jsx",
    "jsxImportSource": "react"
}
}